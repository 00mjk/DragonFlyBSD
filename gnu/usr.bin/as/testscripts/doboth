#!/bin/sh
# $FreeBSD: src/gnu/usr.bin/as/testscripts/doboth,v 1.4 1999/08/27 23:34:34 peter Exp $
# $DragonFly: src/gnu/usr.bin/as/testscripts/Attic/doboth,v 1.2 2003/06/17 04:25:44 dillon Exp $

x=$1 ; shift
y=$1 ; shift

rm tmp.0 > /dev/null 2>&1
ln -s $x tmp.0
$* tmp.0 > tmp.1

rm tmp.0
ln -s $y tmp.0
$* tmp.0 > tmp.2

rm tmp.0

diff -c tmp.1 tmp.2
exit

#eof
