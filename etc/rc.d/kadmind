#!/bin/sh
#
# $FreeBSD: src/etc/rc.d/kadmind,v 1.3 2003/03/08 09:50:10 markm Exp $
# $DragonFly: src/etc/rc.d/kadmind,v 1.4 2006/10/24 09:24:15 victor Exp $
#

# PROVIDE: kadmin
# REQUIRE: kerberos
# BEFORE: DAEMON

. /etc/rc.subr

name="kadmind5"
load_rc_config $name
rcvar="kadmind5_server_enable"
unset start_cmd
command="${kadmind5_server_program}"
command_args="&"
required_vars="kerberos5_server_enable"

run_rc_command "$1"
